<div class ="thankyou content container-fluid">
  <div class="container"> 
    <div class="col-md-6 col-md-offset-3">
      <% flash.each do |key, value| %>
        <%= content_tag :div,value, class: "alert alert-#{key}" %>
      <%end%> 
    </div>
  </div>
<div class="container-fluid">      
  <div class="row"> 
    <div class="col-md-4 col-md-offset-4">
      <div class="notify check ">
        <h1>Thank you!</h1>
          <div class="check-icon">
            
            <%= image_tag("check.png", size: "132x132")  %>
            
            
          </div>  
        <h4>We received your message.</h4>  
        
        <h4>Keep an eye on your inbox. We'll visit your website, rate it and get back to you with valuable insights within the next
        24 hours! </h4>
        
        <button type="button" class="btn btn-primary btn-lg back2"><%= link_to 'Go Back', root_path %></button>
       </div>
    </div>   
  </div>
</div>  
  <%= render "layouts/footer"%>
</div>


