
  <%= form_for @contact,  html: { id: 'my_form1' } do |f| %>
  <div class="col-md-4 col-md-offset-4">
     
    <div class="form-group">
        <%= f.text_field :name, class: 'form-control transparent', placeholder: 'Name', required: true %>
      </div> 
      
      <div class="form-group">
        <%= f.email_field :email, class: 'form-control transparent', placeholder: 'Email', required: true %>
      </div> 
      
      <div class="form-group">
        <%= f.url_field :url, class: 'form-control transparent', placeholder: '(ie.:http://mywebsite.com)', required: true %>
    </div>  
  
        <%= f.submit :'Analyze my website', class: "btn btn-success submission", required: true  %>
  </div>
  <% end %>
<script>
  $("#my_form1").validate();
</script>